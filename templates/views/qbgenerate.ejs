                <div class="mt-0 m-3">
                    <form action="/qbgenerate" onsubmit="checkForm()" method="POST">
                        <div class="pl-5 ml-5">
                            <div class="row">
                                <div class="col-md-6 mb-3 p-1">
                                    <label for="inputlg">University/Board</label>
                                    <input  type="text" class="form-control bg-white col-md-12 input-group-lg" name="university" id="inputlg" value="Mumbai University"
                                        placeholder="University/Board Name" required disabled>
                                </div>
                                <div class="col-md-5 mb-3 p-1">
                                    <label for="validationTooltip02">Branch</label>
                                    <div class="dropdown">
                                        <select class="custom-select col-10 text-muted pl-4" style="height:3rem; border:none;" id="branch" name="branch" required>
                                            <option value="">Choose Branch</option>
                                            <% totalbranch.forEach(branch=>{ %> 
                                            <option value="<%=branch %>"><%=branch  %> </option>
                                            <% }) %> 
                                          </select>
                                    </div>
                                </div>
                                
                            </div>
                            <div class="row">
                                <div class="col-md-5 mb-3 mt-4 p-1">
                                    <label for="validationTooltip02">Subject</label>
                                    <div class="dropdown"> 
                                        <select class="custom-select col-10 text-muted p-2 pl-4" style="height:3rem; border:none;" id="subject" name="subject" required>
                                            <option value="">Choose Branch</option>
                                            <option value="1">One</option>
                                            <option value="2">Two</option>
                                            <option value="3">Three</option>
                                          </select>
                                    </div>
                                </div>
                                
                            </div>
                            <div class="row" id="q1">
                                    <div class="col-md-6 mt-4 p-1">
                                        <label for="validationTooltip01">Total Number of Questions</label>
                                        <input type="number" id="total" class="form-control bg-white"
                                            id="totalquestions" name="totalquestions" value="" placeholder="Total Number of Questions"
                                            required>
                                    </div>
                                    <div class="col-md-6 mt-4 p-1">
                                        <label for="validationTooltip01">Number of Easy Questions</label>
                                        <input type="number" id="easy" class="form-control bg-white"
                                            id="easyquestions" name="easyquestions" value="" placeholder="Number of Easy Questions" onchange="easyQuest()" required>
                                    </div>
                            </div>
                            <div class="row" id="q2">
                                <div class="col-md-6 mt-5 p-1">
                                    <label for="validationTooltip01">Number of Moderate Questions</label>
                                    <input type="number" id="moderate" class="form-control bg-white"
                                        id="moderatequestions" name="moderatequestions" value="" placeholder="Number of Moderate Questions" onchange="easyQuest()"
                                        required>
                                </div>
                                <div class="col-md-6 mt-5 p-1">
                                    <label for="validationTooltip01">Number of Hard Questions</label>
                                    <input type="number" id="hard" class="form-control bg-white"
                                        id="hardquestions" name="hardquestions" value="" placeholder="Number of Hard Questions" onchange="easyQuest()" required>
                                </div>
                            </div>
                        </div>
                </div>
                <div>
                    <div class="dashboard-btn">
                        <label for="errormsg" id="errmsg" style="visibility:hidden;color:red;display:block;">*Total Number of Questions Exceeded!!</label>
                        <button type="submit" class="btn" id="submitbtn" style="background-color: #6206a8; color: white">
                            Generate Question Paper
                        </button>
                    </div>
                </div>
